{
  "Lib": "bbbfly-cjs-morph",
  "Version": "1.0.0",
  "Name": "3b-fly's skin base for Controls.js",
  "Copyright": "Jan Nejedly",

  "RequiredLibraries": {
    "controls.js": "5.0.0",
    "bbbfly-cjs": "2.0.0",
    "bbbfly-cjs-controls": "2.0.0",
    "bbbfly-cjs-morph": "1.0.0"
  },

  "Packages": {
    "core": {
      "DebugFiles": [
        "core/morph.js",
        "core/misc.js"
      ],
      "ReleaseFiles": [
        "core/morph.js",
        "core/misc.js"
      ],
      "Libraries": {
        "controls.js": {
          "Packages": ["Controls.js"]
        },
        "bbbfly-cjs": {
          "Packages": ["crossbrowser"]
        }
      }
    },
    "panel": {
      "DebugFiles": [
        "base/panel.js"
      ],
      "ReleaseFiles": [
        "base/panel.js"
      ],
      "Libraries": {
        "bbbfly-cjs-controls": {
          "Packages": ["panel"]
        },
        "bbbfly-cjs-morph": {
          "Packages": ["core"]
        }
      }
    },
    "grid": {
      "DebugFiles": [
        "base/grid.js"
      ],
      "ReleaseFiles": [
        "base/grid.js"
      ],
      "Libraries": {
        "bbbfly-cjs-controls": {
          "Packages": ["grid"]
        },
        "bbbfly-cjs-morph": {
          "Packages": ["core"]
        }
      }
    },
    "wrapper": {
      "DebugFiles": [
        "base/wrapper.js"
      ],
      "ReleaseFiles": [
        "base/wrapper.js"
      ],
      "Libraries": {
        "bbbfly-cjs-controls": {
          "Packages": ["wrapper"]
        },
        "bbbfly-cjs-morph": {
          "Packages": ["core"]
        }
      }
    }
  }
}
